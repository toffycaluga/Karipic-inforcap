

<h1>Photos</h1>
<% if current_user.is_karina? %>
<%= link_to "New photo", new_photo_path %>
<%end%>
  <div id="photos" class="row row-cols-1 text-center g-4">

    <% @photos.each do |photo| %>
      <div class="col-12">
        <%= render photo,photo:photo %>
      </div>
    <% end %>
  
  </div>


